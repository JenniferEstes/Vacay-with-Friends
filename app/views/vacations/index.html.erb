<h1>All Vacations</h1>

<table class="vacation-table">
  <tr>
    <th>Date</th>
    <th>Vacation</th>
  </tr>
  <% @vacations.each do |v| %>
    <tr>
      <td><%= v.date_traveling %></td>
      <td><%= v.description %></td>
      <td><%= link_to "Edit", edit_user_vacation_path %></td>
    </tr>
  <% end %>
</table>